<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f5"
    android:orientation="vertical">
    <include layout="@layout/layout_title_detail"/>
    <RelativeLayout
        android:id="@+id/rl_message"
        android:background="@drawable/selector_user_center"
        android:layout_marginTop="@dimen/dp_7"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_60">
        <ImageView
            android:layout_marginStart="@dimen/dp_16"
            android:layout_centerVertical="true"
            android:src="@mipmap/ic_message"
            android:layout_width="@dimen/dp_40"
            android:layout_height="@dimen/dp_40"
            android:id="@+id/imageView" />
        <TextView
            android:textColor="#333"
            android:textSize="@dimen/sp_13"
            android:gravity="center"
            android:layout_marginStart="@dimen/dp_10"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/imageView"
            android:text="官方消息" />
        <ImageView
            android:background="@drawable/shape_message"
            android:layout_marginEnd="@dimen/dp_50"
            android:layout_alignParentEnd="true"
            android:layout_width="@dimen/dp_7"
            android:layout_height="@dimen/dp_7"
            android:layout_centerVertical="true" />
        <ImageView
            android:id="@+id/iv_right"
            android:layout_centerVertical="true"
            android:layout_marginEnd="@dimen/dp_10"
            android:layout_alignParentEnd="true"
            android:src="@mipmap/ic_right"
            android:layout_width="@dimen/dp_20"
            android:layout_height="@dimen/dp_20"/>
    </RelativeLayout>

    <RelativeLayout
        android:background="@drawable/selector_user_center"
        android:id="@+id/rl_user_wallet"
        android:layout_marginTop="@dimen/dp_12"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_44">
        <TextView
            android:textColor="#333"
            android:textSize="@dimen/sp_13"
            android:gravity="center"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:text="我的钱包" />

        <ImageView
            android:layout_centerVertical="true"
            android:layout_marginEnd="@dimen/dp_10"
            android:layout_alignParentEnd="true"
            android:src="@mipmap/ic_right"
            android:layout_width="@dimen/dp_20"
            android:layout_height="@dimen/dp_20"/>
    </RelativeLayout>
    <RelativeLayout
        android:background="@drawable/selector_user_center"
        android:id="@+id/rl_user_subscribe"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_44">
    <TextView
        android:textColor="#333"
        android:textSize="@dimen/sp_13"
        android:gravity="center"
        android:layout_marginStart="@dimen/dp_15"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:text="已报名" />
    <TextView
        android:textColor="#999"
        android:textSize="@dimen/sp_13"
        android:text="时间"
        android:layout_centerVertical="true"
        android:layout_marginEnd="@dimen/dp_50"
        android:layout_alignParentEnd="true"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dp_20"/>
        <ImageView
            android:layout_centerVertical="true"
            android:layout_marginEnd="@dimen/dp_10"
            android:layout_alignParentEnd="true"
            android:src="@mipmap/ic_right"
            android:layout_width="@dimen/dp_20"
            android:layout_height="@dimen/dp_20"/>
    </RelativeLayout>
    <RelativeLayout
        android:background="@drawable/selector_user_center"
        android:id="@+id/rl_user_info"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_44">
        <TextView
            android:textColor="#333"
            android:textSize="@dimen/sp_13"
            android:gravity="center"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:text="个人信息" />
        <TextView
            android:textColor="#999"
            android:textSize="@dimen/sp_13"
            android:text="修改"
            android:layout_centerVertical="true"
            android:layout_marginEnd="@dimen/dp_50"
            android:layout_alignParentEnd="true"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_20"/>

        <ImageView
            android:layout_centerVertical="true"
            android:layout_marginEnd="@dimen/dp_10"
            android:layout_alignParentEnd="true"
            android:src="@mipmap/ic_right"
            android:layout_width="@dimen/dp_20"
            android:layout_height="@dimen/dp_20"/>
    </RelativeLayout>



    <RelativeLayout
        android:id="@+id/rl_user_address"
        android:background="@drawable/selector_user_center"
        android:layout_marginTop="@dimen/dp_1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_44">
        <TextView
            android:textColor="#333"
            android:textSize="@dimen/sp_13"
            android:gravity="center"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:text="地址管理" />
        <ImageView
            android:layout_centerVertical="true"
            android:layout_marginEnd="@dimen/dp_10"
            android:layout_alignParentEnd="true"
            android:src="@mipmap/ic_right"
            android:layout_width="@dimen/dp_20"
            android:layout_height="@dimen/dp_20"/>
    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/rl_user_order"
        android:background="@drawable/selector_user_center"
        android:layout_marginTop="@dimen/dp_1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_44">
        <TextView
            android:textColor="#333"
            android:textSize="@dimen/sp_13"
            android:gravity="center"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:text="我的订单" />
        <ImageView
            android:layout_centerVertical="true"
            android:layout_marginEnd="@dimen/dp_10"
            android:layout_alignParentEnd="true"
            android:src="@mipmap/ic_right"
            android:layout_width="@dimen/dp_20"
            android:layout_height="@dimen/dp_20"/>
    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/rl_real_name"
        android:background="@drawable/selector_user_center"
        android:layout_marginTop="@dimen/dp_1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_44">
        <TextView
            android:textColor="#333"
            android:textSize="@dimen/sp_13"
            android:gravity="center"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:text="实名认证" />
        <TextView
            android:textColor="#999"
            android:textSize="@dimen/sp_13"
            android:text="未认证"
            android:layout_centerVertical="true"
            android:layout_marginEnd="@dimen/dp_50"
            android:layout_alignParentEnd="true"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_20"/>
        <ImageView
            android:layout_centerVertical="true"
            android:layout_marginEnd="@dimen/dp_10"
            android:layout_alignParentEnd="true"
            android:src="@mipmap/ic_right"
            android:layout_width="@dimen/dp_20"
            android:layout_height="@dimen/dp_20"/>
    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/rl_update_psd"
        android:background="@drawable/selector_user_center"
        android:layout_marginTop="@dimen/dp_14"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_44">
        <TextView
            android:textColor="#333"
            android:textSize="@dimen/sp_13"
            android:gravity="center"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:text="修改密码" />

        <ImageView
            android:layout_centerVertical="true"
            android:layout_marginEnd="@dimen/dp_10"
            android:layout_alignParentEnd="true"
            android:src="@mipmap/ic_right"
            android:layout_width="@dimen/dp_20"
            android:layout_height="@dimen/dp_20"/>
    </RelativeLayout>




</LinearLayout>